/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=notab);
    Nand(a=a, b=notab, out=not1);
    Nand(a=notab, b=b, out=not2);
    Nand(a=not1, b=not2, out=out);
}